<component name="ProjectRunConfigurationManager">
  <configuration name="{CONFIG_NAME}" type="tests" factoryName="py.test">
    <module name="{MODULE_NAME}" />
    <option name="INTERPRETER_OPTIONS" value="" />
    <option name="PARENT_ENVS" value="true" />
    <envs>
      {ENV_VARS}
    </envs>
    <option name="SDK_HOME" value="" />
    <option name="WORKING_DIRECTORY" value="{WORKING_DIR}" />
    <option name="IS_MODULE_SDK" value="true" />
    <option name="ADD_CONTENT_ROOTS" value="true" />
    <option name="ADD_SOURCE_ROOTS" value="true" />
    <option name="_new_keywords" value="&quot;&quot;" />
    <option name="_new_parameters" value="&quot;&quot;" />
    <option name="_new_additionalArguments" value="{PYTEST_OPTIONS}" />
    <option name="_new_target" value="&quot;{TEST_TARGET}&quot;" />
    <option name="_new_targetType" value="&quot;{TARGET_TYPE}&quot;" />
    <method v="2" />
  </configuration>
</component>
